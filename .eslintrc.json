{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": ["error"],
        "no-console" : "off",
        "no-var" : "error",
        "init-declarations": ["error", "never"],
        "no-plusplus":"off"
    }
}
